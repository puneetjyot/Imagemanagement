<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>RegisterServlet</servlet-name>
  	<display-name>RegisterServlet</display-name>
  	<description></description>
  	<servlet-class>com.nagarro.register.RegisterServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>LoginServlet</servlet-name>
  	<display-name>LoginServlet</display-name>
  	<description></description>
  	<servlet-class>com.nagarro.Login.LoginServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>FileUploadDBServlet</servlet-name>
  	<display-name>FileUploadDBServlet</display-name>
  	<description></description>
  	<servlet-class>com.nagarro.fileupload.FileUploadDBServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>DeleteImageServlet</servlet-name>
  	<display-name>DeleteImageServlet</display-name>
  	<description></description>
  	<servlet-class>com.nagarro.DeleteImage.DeleteImageServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>updateNameServlet</servlet-name>
  	<display-name>updateNameServlet</display-name>
  	<description></description>
  	<servlet-class>com.nagarro.updateName.updateNameServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>UpdateImageServlet</servlet-name>
  	<display-name>UpdateImageServlet</display-name>
  	<description></description>
  	<servlet-class>com.nagarro.updateimage.UpdateImageServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>UpdateImage</servlet-name>
  	<display-name>UpdateImage</display-name>
  	<description></description>
  	<servlet-class>com.nagarro.services.UpdateImage</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>RegisterServlet</servlet-name>
  	<url-pattern>/RegisterServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LoginServlet</servlet-name>
  	<url-pattern>/LoginServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>FileUploadDBServlet</servlet-name>
  	<url-pattern>/FileUploadDBServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DeleteImageServlet</servlet-name>
  	<url-pattern>/DeleteImageServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>updateNameServlet</servlet-name>
  	<url-pattern>/updateNameServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>UpdateImageServlet</servlet-name>
  	<url-pattern>/UpdateImageServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>UpdateImage</servlet-name>
  	<url-pattern>/UpdateImage</url-pattern>
  </servlet-mapping>
</web-app>
